<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="lamen.cipher.MainActivity"    android:background="#FAFAFA">    <!--<me.dm7.barcodescanner.zbar.ZBarScannerView-->        <!--android:id="@+id/zbar"-->        <!--android:layout_width="match_parent"-->        <!--android:layout_height="match_parent"-->        <!--android:layout_marginBottom="10dp"-->        <!--android:layout_marginTop="10dp">-->    <!--</me.dm7.barcodescanner.zbar.ZBarScannerView>-->        <cn.bingoogolapple.qrcode.zbar.ZBarView            android:id="@+id/scan_camera_view"            app:qrcv_animTime="1000"            app:qrcv_borderColor="#333333"            app:qrcv_borderSize="1dp"            app:qrcv_cornerColor="#5D6565"            app:qrcv_cornerLength="20dp"            app:qrcv_cornerSize="3dp"            app:qrcv_isShowDefaultScanLineDrawable="true"            app:qrcv_maskColor="#33000000"            app:qrcv_rectWidth="500dp"            app:qrcv_scanLineColor="#777777"            app:qrcv_topOffset="0dp"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_marginBottom="7dp"            android:layout_marginTop="7dp" />    <RelativeLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:id="@+id/scan_success_relative"        android:visibility="gone">        <RelativeLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:id="@+id/scan_success_ok_relative"            android:layout_alignParentTop="true"            android:layout_alignParentLeft="true"            android:layout_alignParentStart="true">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="  "                android:id="@+id/scan_success_devider"                android:background="#111111"                android:paddingLeft="3dp"                android:paddingRight="3dp"                android:paddingTop="3dp"                android:paddingBottom="3dp"                android:visibility="visible"                android:textColor="#FFFFFF"                android:layout_centerVertical="true"                android:layout_toLeftOf="@+id/scan_success_text"                android:layout_toStartOf="@+id/scan_success_text" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="@string/scan_success"                android:id="@+id/scan_success_text"                android:background="#111111"                android:paddingTop="3dp"                android:paddingBottom="3dp"                android:visibility="visible"                android:textColor="#FFFFFF"                android:layout_centerVertical="true"                android:layout_toRightOf="@+id/scan_success_image" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:id="@+id/scan_success_image"                android:src="@mipmap/generate_scan_ok"                android:layout_centerVertical="true"                android:layout_alignParentLeft="true"                android:layout_alignParentStart="true" />        </RelativeLayout>        <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="@string/rescan"                android:id="@+id/scan_rescan_text"                android:background="#111111"                android:paddingLeft="3dp"                android:paddingRight="3dp"                android:paddingTop="3dp"                android:paddingBottom="3dp"                android:visibility="visible"                android:textColor="#FFFFFF"            android:layout_below="@+id/scan_success_ok_relative"            android:layout_alignRight="@+id/scan_success_ok_relative"            android:layout_alignEnd="@+id/scan_success_ok_relative" />    </RelativeLayout></RelativeLayout>